<Window x:Class="Lab7.baz.lvl._15var.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab7.baz.lvl._15var"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBox x:Name="txtNumber" Width="200" Margin="5"/>
            <Button Content="В очереди" Width="200" Margin="5" Click="Enqueue_Click"/>
            <Button Content="Вне очереди" Width="200" Margin="5" Click="Dequeue_Click"/>
            <Button Content="Минимальное" Width="200" Margin="5" Click="FindMin_Click"/>
            <ListBox x:Name="lstQueue" Width="200"/>
        </StackPanel>
    </Grid>
</Window>